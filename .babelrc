{
  "presets": [
    "es2015",
    "stage-3",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }],
    [
      "syntax-dynamic-import"
    ],
    ["react-loadable/babel"],
    ["transform-decorators-legacy"]
  ]
}
